{
	"extends": [
		"defaults"
	],
	"env": {
		"node": true,
		"es6": true,
		"jasmine": true
	},
	"rules": {
		"comma-dangle": [
			2,
			"only-multiline"
		],
		"no-control-regex": 2,
		"no-debugger": 2,
		"no-dupe-args": 2,
		"no-dupe-keys": 2,
		"no-duplicate-case": 2,
		"no-empty-character-class": 2,
		"no-ex-assign": 2,
		"no-extra-boolean-cast": 2,
		"no-extra-parens": [
			2,
			"functions"
		],
		"no-extra-semi": 2,
		"no-func-assign": 2,
		"no-invalid-regexp": 2,
		"no-irregular-whitespace": 2,
		"no-negated-in-lhs": 2,
		"no-obj-calls": 2,
		"no-proto": 2,
		"no-unexpected-multiline": 2,
		"no-unreachable": 2,
		"use-isnan": 2,
		"valid-typeof": 2,
		"no-fallthrough": 2,
		"no-octal": 2,
		"no-redeclare": 2,
		"no-self-assign": 2,
		"no-unused-labels": 2,
		"strict": [
			2,
			"global"
		],
		"no-delete-var": 2,
		"no-undef": 2,
		"no-unused-vars": [
			1,
			{
				"args": "after-used"
			}
		],
		"no-mixed-requires": 2,
		"no-new-require": 2,
		"no-path-concat": 2,
		"no-restricted-modules": [
			2,
			"sys",
			"_linklist"
		],
		"comma-spacing": 2,
		"eol-last": 2,
		"indent": [
			2,
			4,
			{
				"SwitchCase": 1
			}
		],
		"keyword-spacing": 2,
		"max-len": [
			2,
			120,
			2
		],
		"new-parens": 2,
		"no-mixed-spaces-and-tabs": 2,
		"no-multiple-empty-lines": [
			2,
			{
				"max": 2
			}
		],
		"no-trailing-spaces": 2,
		"quotes": [
			2,
			"double",
			"avoid-escape"
		],
		"semi": 2,
		"space-before-blocks": [
			2,
			"always"
		],
		"space-before-function-paren": [
			2,
			"never"
		],
		"space-in-parens": [
			2,
			"never"
		],
		"space-infix-ops": 2,
		"space-unary-ops": 2,
		"no-warning-comments": 1,
		"max-depth": [
			1,
			3
		],
		"max-statements": [
			1,
			15
		],
		"camelcase": [
			2,
			{
				"properties": "never"
			}
		],
		"brace-style": [
			2,
			"1tbs",
			{
				"allowSingleLine": true
			}
		],
		"block-scoped-var": 2,
		"curly": 2,
		"eqeqeq": 2,
		"no-extend-native": 2,
		"no-use-before-define": 2,
		"key-spacing": [
			2,
			{
				"beforeColon": false,
				"afterColon": true
			}
		],
		"object-curly-spacing": [
			2,
			"never"
		],
		"no-underscore-dangle": [
			2,
			{
				"allow": [
					"_id",
					"_"
				]
			}
		]
	}
}